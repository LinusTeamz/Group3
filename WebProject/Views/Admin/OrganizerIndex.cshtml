@model IEnumerable<WebProject.Models.Organizer>

@{
    ViewBag.Title = "OrganizerIndex";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>Arrangörer</h2>

<p>
    @Html.ActionLink("Skapa ny", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Phone)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Phone)
            </td>
            <td>
                @Html.ActionLink("Ändra", "OrganizerEdit", new { id = item.Id }) |
                @Html.ActionLink("Radera", "OrganizerDelete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
